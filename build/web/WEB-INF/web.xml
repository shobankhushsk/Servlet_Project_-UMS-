<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>FacultyServlet</servlet-name>
        <servlet-class>com.Servlets.FacultyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DepartmentServlet</servlet-name>
        <servlet-class>com.Servlets.DepartmentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProgramsServlet</servlet-name>
        <servlet-class>com.Servlets.ProgramsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProgServlet</servlet-name>
        <servlet-class>com.Servlets.ProgServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetProgramsServlet</servlet-name>
        <servlet-class>com.Servlets.GetProgramsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetDepts</servlet-name>
        <servlet-class>com.Servlets.GetDepts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BatchServlet</servlet-name>
        <servlet-class>com.Servlets.BatchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Students</servlet-name>
        <servlet-class>com.Servlets.Students</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FacultyServlet</servlet-name>
        <url-pattern>/FacultyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DepartmentServlet</servlet-name>
        <url-pattern>/DepartmentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProgramsServlet</servlet-name>
        <url-pattern>/ProgramsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProgServlet</servlet-name>
        <url-pattern>/ProgServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetProgramsServlet</servlet-name>
        <url-pattern>/GetProgramsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetDepts</servlet-name>
        <url-pattern>/GetDepts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BatchServlet</servlet-name>
        <url-pattern>/BatchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Students</servlet-name>
        <url-pattern>/Students</url-pattern>
    </servlet-mapping>

    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
